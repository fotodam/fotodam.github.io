<!-- Portfolio -->
<a name="myAnchorPortfolio" id="myAnchorPortfolio">
</a>
<section class="sk-portfolio">
    <div class="row">
        <div class="sk-section-header text-center">
            <h2>Galeria</h2>
        </div>
    </div>
    <% _.chunk(site.data.galleries, 4).forEach(function(chunk){ %>
        <div class="row">
            <% chunk.forEach(function(gallery){ %>
                <div class="medium-3 columns sk-portfolio-box wow fadeIn">
                    <a href="<%- url_for(gallery.path) %>">
                        <div class="card">
                            <img src="<%- s3galleryPhotoUrl(gallery.data.gallery.startPhoto, { resize: { width: 800 } }) %>"
                                 alt="<%= gallery.data.gallery.galleryName %>">

                            <div class="sk-portfolio-title text-center">
                                <h4><%= gallery.data.gallery.galleryName %></h4>
                                <div class="small button" href="<%- url_for(gallery.path) %>">
                                    Zobacz
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
            <% }) %>
        </div>
    <% }) %>
    </div>
</section>


